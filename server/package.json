{
  "name": "express-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client-install": "npm install --prefix ../client",
    "start": "node ./bin/www",
    "devStart": "nodemon ./bin/www",
    "serverstart": "DEBUG=express-locallibrary-tutorial:* npm run devstart",
